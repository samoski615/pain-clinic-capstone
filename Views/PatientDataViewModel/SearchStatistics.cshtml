<!DOCTYPE HTML>
<html>
<head>
    <script type="text/javascript">
        window.onload = function () {

            var chart1 = new CanvasJS.Chart("chartContainer1", {
                theme: "dark2", // "light2", "dark1", "dark2"
                animationEnabled: true, // change to true
                title: {
                    text: "Pain Ratings/Average Past 30 Days"
                },
                data: [
                    {
                        // Change type to "bar", "area", "spline", "pie",etc.
                        type: "column",
                        dataPoints: [
                            { label: "1", y: @ViewBag.ratingOne },
                            { label: "2", y: @ViewBag.ratingTwo },
                            { label: "3", y: @ViewBag.ratingThree },
                            { label: "4", y: @ViewBag.ratingFour },
                            { label: "5", y: @ViewBag.ratingFive }
                        ]
                    }
                ]
            });
            chart1.render();


             var chart2 = new CanvasJS.Chart("chartContainer2", {
                theme: "light1", // "light2", "dark1", "dark2"
                animationEnabled: true, // change to true
                title: {
                    text: "Pain Location/Average Past 30 Days"
                },
                data: [
                    {
                        // Change type to "bar", "area", "spline", "pie",etc.
                        type: "pie",
                        dataPoints: [
                            { label: "Upper Back", y: @ViewBag.ratingOne },
                            { label: "Lower Back", y: @ViewBag.ratingTwo },
                            { label: "Neck", y: @ViewBag.ratingThree },
                            { label: "Head", y: @ViewBag.ratingFour },
                            { label: "Legs", y: @ViewBag.ratingFive }
                            { label: "Arms", y: @ViewBag.ratingFive }
                            { label: "Shoulders", y: @ViewBag.ratingFive }
                        ]
                    }
                ]
            });
            chart2.render();

             var chart3 = new CanvasJS.Chart("chartContainer3", {
                theme: "light1", // "light2", "dark1", "dark2"
                animationEnabled: true, // change to true
                title: {
                    text: "Pain Ratings/Average Past 30 Days"
                },
                data: [
                    {
                        // Change type to "bar", "area", "spline", "pie",etc.
                        type: "column",
                        dataPoints: [
                            { label: "1", y: @ViewBag.ratingOne },
                            { label: "2", y: @ViewBag.ratingTwo },
                            { label: "3", y: @ViewBag.ratingThree },
                            { label: "4", y: @ViewBag.ratingFour },
                            { label: "5", y: @ViewBag.ratingFive }
                        ]
                    }
                ]
            });
            chart3.render();

             var chart4 = new CanvasJS.Chart("chartContainer4", {
                theme: "light1", // "light2", "dark1", "dark2"
                animationEnabled: true, // change to true
                title: {
                    text: "Pain Ratings/Average Past 30 Days"
                },
                data: [
                    {
                        // Change type to "bar", "area", "spline", "pie",etc.
                        type: "column",
                        dataPoints: [
                            { label: "1", y: @ViewBag.ratingOne },
                            { label: "2", y: @ViewBag.ratingTwo },
                            { label: "3", y: @ViewBag.ratingThree },
                            { label: "4", y: @ViewBag.ratingFour },
                            { label: "5", y: @ViewBag.ratingFive }
                        ]
                    }
                ]
            });
            chart4.render();
        }
    </script>
</head>
<body>
    <br /><br />
    <div id="chartContainer1" style="height: 370px; width: 100%;"></div>
    <br /><br />
    <div id="chartContainer2" style="height: 370px; width: 100%;"></div>
    <br /><br />
    <div id="chartContainer3" style="height: 370px; width: 100%;"></div>
    <br /><br />
    <div id="chartContainer4" style="height: 370px; width: 100%;"></div>
    <script src="https://canvasjs.com/assets/script/canvasjs.min.js"></script>
</body>
</html>