@{
    ViewData["Title"] = "Clinic Location";
}
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie-edge">
    @*<title> My Google Map</title>*@
    <style>
        #map {
            height: 600px;
            width: 80%;
        }

        html, body {
            height: 400%;
            margin: 0;
            padding: 0;
        }
    </style>
</head>
<body>
    @*<h2></h2>*@
    <div id="map"></div>
    <script>

      


        function initMap() {
            var apm = {lat: 42.958280, lng:-87.971130}
            var map = new google.maps.Map(document.getElementById('map'), {
                center: { lat: 43.0389, lng: -87.9065 },
                zoom: 8,
            });
            var marker = new google.maps.Marker({ position: apm, map: map });
        }
    </script>
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyA7WCLwfQaC05-2fLAES86p1dWC84xfL4c&callback=initMap"
            async defer></script>

</body>
</html>





