
@{
    ViewData["Title"] = "Clinic Map";
}
<!DOCTYPE html>
<html>

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie-edge">
    <title> My Google Map</title>
    <style>
        #map {
            height: 400px;
            width: 80%;
        }

        html, body {
            height: 400%;
            margin: 0;
            padding: 0;
        }
    </style>
</head>

<body>
    <h2>GoogleMapAPI</h2>
    <div id="map"></div>

    <script>
        function initMap() {
            var dCC = { lat: 43.034271, lng: -87.911919 }
            var map = new google.maps.Map(document.getElementById("map"), {
                center: { lat: 43.0389, lng: -87.9065 },
                zoom: 8,
                center: dCC
            });
            var marker = new google.maps.Marker({
                position: dCC,
                map: map
            });
        }
    </script>
    <script async defer src="https://maps.googleapis.com/maps/api/js?key=AIzaSyABkqcRJsP0cZpP3GTYyZDeTxC3iaUDoMQ&callback=initMap"></script>

</body>
</html>




